{
    "name": "davide-casiraghi/laravel-columns",
    "description": "A Laravel library to generate responsive columns with multi language contents.",
    "keywords": [
        "davide-casiraghi",
        "laravel-columns"
    ],
    "homepage": "https://github.com/davide-casiraghi/laravel-columns",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Davide Casiraghi",
            "email": "davide.casiraghi@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "astrotomic/laravel-translatable": "^11.1",
        "davide-casiraghi/laravel-form-partials": "^1.0",
        "illuminate/support": "5.8.* || ^6.0",
        "intervention/image": "^2.4",
        "mcamara/laravel-localization": "^1.3",
        "nesbot/carbon": "^2.19"
    },
    "require-dev": {
        "orchestra/testbench": "3.8.*",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "DavideCasiraghi\\LaravelColumns\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DavideCasiraghi\\LaravelColumns\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "DavideCasiraghi\\LaravelColumns\\LaravelColumnsServiceProvider"
            ],
            "aliases": {
                "LaravelColumns": "DavideCasiraghi\\LaravelColumns\\LaravelColumns"
            }
        }
    }
}
